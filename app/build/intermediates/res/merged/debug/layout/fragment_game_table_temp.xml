<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    tools:context="com.example.itachi.bullsandcows.GameTableTempFragment"
    android:foregroundGravity="center_horizontal">

    <!-- TODO: Update blank fragment layout -->

    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center_horizontal"
        android:foregroundGravity="center_horizontal"
        android:showDividers="beginning|middle|end"
        android:layout_gravity="center_horizontal">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:text="Prev Try Results"
            android:id="@+id/headingView"
            android:layout_row="0"
            android:layout_column="0"
            android:layout_gravity="center_horizontal"
            android:layout_columnSpan="4"
            android:textSize="25sp" />


        <TableLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

            <TableRow
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="S No"
                    android:id="@+id/sNoContent"
                    android:layout_row="2"
                    android:layout_column="0"
                    android:layout_gravity="center_horizontal"
                    android:layout_rowSpan="3"
                    android:scrollIndicators="bottom|top"

                    android:textSize="20sp"
                    android:padding="6dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Guessed no"
                    android:id="@+id/guessNoContent"
                    android:layout_row="2"
                    android:layout_column="1"
                    android:layout_gravity="center_horizontal"
                    android:layout_rowSpan="3"
                    android:scrollIndicators="bottom|top"
                    android:textSize="20sp"
                    android:padding="6dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="Bulls"
                    android:id="@+id/bullsPrevContent"
                    android:layout_row="2"
                    android:layout_column="2"
                    android:layout_gravity="center_horizontal"
                    android:layout_rowSpan="3"
                    android:scrollIndicators="bottom|top"
                    android:textSize="20sp"
                    android:padding="6dp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="40dp"
                    android:textAppearance="?android:attr/textAppearanceLarge"
                    android:text="cows"
                    android:id="@+id/cowsPrevContent"
                    android:layout_row="2"
                    android:layout_column="3"
                    android:layout_gravity="center_horizontal"
                    android:layout_rowSpan="3"
                    android:scrollIndicators="bottom|top"
                    android:textSize="20sp"
                    android:padding="6dp" />



            </TableRow>
        </TableLayout>

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


        <TableLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal|top"
            android:nestedScrollingEnabled="true"
            android:id="@+id/gameTableLayout"
            android:divider="#1d1919"
            android:showDividers="beginning|middle|end">

        </TableLayout>

        </ScrollView>

    </LinearLayout>

</FrameLayout>
